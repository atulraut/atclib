#include <stdio.h>

typedef struct node {
  int value;
  struct node *right;
  struct node *left;
} mynode;

mynode *root;
add_node(int value);
void levelOrderTraversal(mynode *root);

int main(int argc, char* argv[]) {
  root = NULL;
  add_node(5);
  add_node(1);
  add_node(-20);
  add_node(100);
  add_node(23);
  add_node(67);
  add_node(13);
  printf("\n\n\nLEVEL ORDER TRAVERSAL\n\n");
  levelOrderTraversal(root);
}
